{
    "printWidth": 130,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "importOrder": ["^tests/mocks/", "^pages/|^router/|^shared/|^static/|^tests/", "^[./]"],
    "importOrderSeparation": true
}
